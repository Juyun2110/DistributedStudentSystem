<Window x:Class="DesktopClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox Name="StudentName" HorizontalAlignment="Left" Height="28" Margin="292,70,0,0" TextWrapping="NoWrap" Text="Name" VerticalAlignment="Top" Width="150"/>
        <TextBox Name="StudentId" HorizontalAlignment="Left" Height="28" Margin="292,130,0,0" TextWrapping="NoWrap" Text="ID" VerticalAlignment="Top" Width="150"/>
        <TextBox Name="StudentUni" HorizontalAlignment="Left" Height="28" Margin="292,189,0,0" TextWrapping="NoWrap" Text="University" VerticalAlignment="Top" Width="150"/>
        <TextBox Name="StudentIndex" HorizontalAlignment="Left" Height="28" Margin="292,289,0,0" TextWrapping="NoWrap" VerticalAlignment="Top" Width="75"/>
        <Button Name="SearchButton" Content="Search" HorizontalAlignment="Left" Height="26" Margin="423,289,0,0" VerticalAlignment="Top" Click="SearchButton_Click"/>
        <Label Content="Total number of student" HorizontalAlignment="Left" Height="26" Margin="122,289,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
